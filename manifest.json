{
  "manifest_version": 2,

  "name": "Tabby",
  "description": "Organize your tabs",
  "version": "1.0",

  "background": {
    "scripts": ["js/popup.js"]
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
